<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#1E1E1E"
    tools:context=".FragForStory">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_320sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_115sdp"
        android:layout_marginEnd="11dp"
        android:background="#252525"
        android:orientation="horizontal"
        android:visibility="visible" />

    <LinearLayout
        android:layout_width="@dimen/_250sdp"
        android:layout_height="@dimen/_57sdp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="@dimen/_210sdp"
        android:background="@drawable/corner_around"
        android:orientation="vertical"
        android:visibility="visible" />

    <Button
        android:id="@+id/copyButton2"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="295dp"
        android:layout_marginTop="@dimen/_217sdp"
        android:contentDescription="@string/app_name"
        android:isScrollContainer="false"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorLink="#000000"
        android:visibility="visible"
        app:cornerRadius="10dp"
        app:icon="@drawable/copy_icon"
        app:iconGravity="textTop"
        app:iconPadding="0dp"
        app:iconSize="15dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.922"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343"
        app:rippleColor="#000000" />

    <TextView
        android:id="@+id/eeee3"
        android:layout_width="@dimen/_190sdp"
        android:layout_height="@dimen/_35sdp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/_35sdp"
        android:layout_marginTop="@dimen/_220sdp"
        android:autoLink="all"
        android:cursorVisible="false"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="..."
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207" />

    <TextView
        android:id="@+id/mainLable2"
        android:layout_width="@dimen/_270sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_150sdp"
        android:fontFamily="sans-serif-light"
        android:text="Url Shorner forever!"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="35sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="@dimen/_155sdp"
        android:layout_height="@dimen/_37sdp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="48dp"
        android:layout_marginTop="@dimen/_315sdp"
        android:layout_marginBottom="@dimen/_274sdp"
        android:background="@drawable/rounded_edittext"
        android:drawablePadding="@dimen/_12sdp"
        android:inputType="textWebEditText|text"
        android:paddingStart="12dp"
        android:text="url"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        android:textIsSelectable="false"
        android:visibility="visible" />

    <CheckBox
        android:id="@+id/checkBoxc3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_392sdp"
        android:text="Capital"
        android:textColor="#FFFFFF"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.636" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_35sdp"
        android:layout_marginTop="@dimen/_402sdp"
        android:src="@drawable/ic_baseline_font_download_24" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="@dimen/_53sdp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_80sdp"
        android:layout_marginEnd="@dimen/_174sdp"
        android:gravity="center"
        android:text="Cipo"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/_37sdp"
        android:layout_height="@dimen/_35sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_190sdp"
        android:layout_marginTop="@dimen/_39sdp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoin" />

    <Button
        android:id="@+id/setButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_202sdp"
        android:layout_marginTop="@dimen/_315sdp"
        android:contentDescription="@string/app_name"
        android:isScrollContainer="false"
        android:text="short"
        android:textColor="#000000"
        android:textColorHighlight="#000000"
        android:textColorLink="#000000"
        android:visibility="visible"
        app:iconGravity="textTop"
        app:iconPadding="0dp"
        app:iconSize="@dimen/_15sdp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.922"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343"
        app:rippleColor="#000000" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/_28sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_marginStart="@dimen/_42sdp"
        android:layout_marginTop="@dimen/_280sdp"
        android:src="@drawable/amountic"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/crew3"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_30sdp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/_75sdp"
        android:layout_marginTop="@dimen/_278sdp"
        android:layout_marginEnd="@dimen/_400sdp"
        android:gravity="center"
        android:shadowDx="15"
        android:shadowDy="15"
        android:shadowRadius="45"
        android:text="text"
        android:textColor="#FDFDFD"
        android:textColorHighlight="@color/white"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/errorViewText"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginStart="@dimen/_65sdp"
        android:layout_marginLeft="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_360sdp"
        android:text="TextView"
        android:textColor="#BA1717"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/iconAnimate"
        android:layout_width="@dimen/_17sdp"
        android:layout_height="@dimen/_19sdp"
        android:layout_marginStart="@dimen/_40sdp"
        android:layout_marginTop="@dimen/_360sdp"
        android:src="@drawable/vd_vector"
        android:visibility="invisible" />


</RelativeLayout>